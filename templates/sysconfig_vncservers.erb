VNCSERVERS="<% @_vncservers.each_with_index do |(vncuser,useroptshash),idx| -%><%= useroptshash.fetch("displaynumber") %>:<%= vncuser %><%= ' ' if idx < (@_vncservers.size - 1) %><% end -%>"
<% @_vncservers.each do |vncuser,useroptshash| -%><% if useroptshash.has_key?("args") -%>VNCSERVERARGS[<%= useroptshash.fetch("displaynumber") %>]="<% useroptshash.fetch("args").each_with_index do |arg,idx| -%>-<%= arg %><%= ' ' if idx < (useroptshash.fetch("args").size - 1) %><% end -%>"<% end -%><% end -%>
